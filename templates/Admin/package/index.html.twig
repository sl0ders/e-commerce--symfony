{% extends "base.html.twig" %}
{% trans_default_domain "NegasProjectTrans" %}
{% block body %}
    <div class="datatable">
        <h5 class="card-header text-center py-4">
            <strong>{{ "package.title.index" | trans }}</strong>
        </h5>
        <div class="datatable_content">
            {{ include('Layout/_datatable.html.twig', {subject : "packaging"}) }}
            <div class="button">
                <a href="{{ path('admin_package_new') }}" class="btn btn-success text-white" title="{{ "button.create"|trans }}">{{ "button.create" | trans }}</a>
                <a href="{{ path('admin_home') }}" class="btn btn-primary float-left" title="{{ "button.return"|trans }}">{{ "button.return" | trans }}</a>
            </div>
        </div>
    </div>
{% endblock %}
