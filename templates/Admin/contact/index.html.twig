{% extends 'base.html.twig' %}
{% trans_default_domain "NegasProjectTrans" %}
{% block title %}{{ "contact.title.index" | trans }}{% endblock %}

{% block body %}
    <div class="datatable">
        <h5 class="card-header text-center py-4">
            <strong>{{ "contact.title.index" | trans }}</strong>
        </h5>
        <div class="datatable_content">
            {{ include('Layout/_datatable.html.twig', {"subject": "contact"}) }}
        </div>
        <div class="button">
            <a href='{{ path('home') }}' class='btn btn-primary float-left'>{{ "button.return"|trans }}</a>
        </div>
    </div>
{% endblock %}
