{% trans_default_domain "NegasProjectTrans" %}
<form method="post" action="{{ path('admin_news_delete', {'id': news.id}) }}" onsubmit="return confirm({{ "return.deleteQuestion" | trans }});">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ news.id) }}">
    <button class="btn btn-danger">{{ "button.delete" | trans }}</button>
</form>
